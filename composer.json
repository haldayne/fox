{
    "name": "haldayne/fox",
    "description": "PHP implementation of generic, higher-order functions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bishop Bettini", "email": "bishop@php.net"
        }
    ],
    "require": {
        "php": "^5.5.0",
        "haldayne/boost": "^1.0-dev"
    },
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "Haldayne\\Fox\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "satooshi/php-coveralls": "^0.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Haldayne\\Fox\\": "tests/"
        }
    }
}
